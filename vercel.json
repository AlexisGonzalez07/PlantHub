{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": {
        "builder": "@vercel/node",
        "command": "npm run build",
        "output": "client/build"
      }
    },
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "client/build/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "server/server.js"
    }
  ]
}